{
  "name": "spotify-api",
  "version": "1.0.0",
  "main": "./dist/src/server.js",
  "license": "MIT",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "dev": "nodemon",
    "build": "tsc"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.5",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "knex": "^0.20.13",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
